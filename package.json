{
  "name": "react-rxjs-hooks",
  "version": "0.0.1",
  "description": "React hooks for use with RxJS",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "typings": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/Fyzu/react-rxjs-hooks.git",
  "author": "Dmitry Petrov <dvpetrov.main@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "rm -rf dist/esm && tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "rm -rf dist/cjs && tsc --module commonjs --target es5 --outDir dist/cjs",
    "lint": "tslint '{src,types}/**/*.ts{,x}'",
    "format": "prettier --write '{src,types}/**/*.ts{,x}'"
  },
  "devDependencies": {
    "@types/react": "^16.8.3",
    "react": "^16.8.2",
    "rxjs": "^6.4.0",
    "typescript": "^3.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "rxjs": "^6.4.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
